static uint_64_t getConvertedDEHBits(uint_64_t bits) {
  static const uint_64_t bitMap[32] = {

    (uint_64_t)(0x0000000000000001),
    (uint_64_t)(0x0000000000000002),
    (uint_64_t)(0x0000000000000004),
    (uint_64_t)(0x0000000000000008),
    (uint_64_t)(0x0000000000000010),
    (uint_64_t)(0x0000000000000020),
    (uint_64_t)(0x0000000000000040),
    (uint_64_t)(0x0000000000000080),
    (uint_64_t)(0x0000000000000100),
    (uint_64_t)(0x0000000000000200),
    (uint_64_t)(0x0000000000000400),
    (uint_64_t)(0x0000000000000800),
    (uint_64_t)(0x0000000000001000),
    (uint_64_t)(0x0000000000002000),
    (uint_64_t)(0x0000000000004000),
    (uint_64_t)(0x0000000000008000),
    (uint_64_t)(0x0000000000010000),
    (uint_64_t)(0x0000000000020000),
    (uint_64_t)(0x0000000000040000),
    (uint_64_t)(0x0000000000080000),
    (uint_64_t)(0x0000000000100000),
    (uint_64_t)(0x0000000000200000),
    (uint_64_t)(0x0000000000400000),
    (uint_64_t)(0x0000000000800000),
    (uint_64_t)(0x0000000001000000),
    (uint_64_t)(0x0000000002000000),
    (uint_64_t)(0x0000000004000000),
    (uint_64_t)(0x0000000008000000),


    (uint_64_t)0x0000000100000000ll,
    (uint_64_t)0x0000000200000000ll,
    (uint_64_t)0x0000000400000000ll,
    (uint_64_t)(0x0000000040000000)
  };
  int i;
  uint_64_t shiftBits = bits;
  uint_64_t convertedBits = 0;
  for (i=0; i<32; i++) {
    if (shiftBits & 0x1) convertedBits |= bitMap[i];
    shiftBits >>= 1;
  }
  return convertedBits;
}




